{
  "name": "estore_ui",
  "version": "1.0.0",
  "description": "eStore UI",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/etherisc/dip-platform"
  },
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "build": "webpack",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "json-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "setupFiles": [
      "<rootDir>/internals/enzyme-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "evergreen-ui": "^4.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "moment": "^2.22.2",
    "polished": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.11",
    "react-router-dom": "^4.3.1",
    "react-stripe-elements": "^2.0.1",
    "sanitize.css": "^8.0.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.0.0",
    "svg-url-loader": "^2.3.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "websocket-as-promised": "^0.9.0"
  }
}
